<link rel="stylesheet" type="text/css" href="/assets/css/favorites.css">

<h1 id="fav">Favorites Playlist</h1>

<div style="overflow-x: auto;">

<table id="table">
    <thead>
        <th> 
            Fav Song Title
        </th>
        <th>
            Artist
        </th>
        
    </thead>
    <tbody>
        {{#each favorite_songs as |song|}}
        <tr>
            <td>{{song.name}}</td>
            <td>{{song.artist}}</td>
        </tr>
        {{/each}}

    </tbody>
</table>
</div>